{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": [
    "google",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "camelcase": 0,
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": false
      }
    ],
    "no-unexpected-multiline": "error"
  }
}
